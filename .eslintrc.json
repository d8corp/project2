{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-return-assign": "off",
    "import/no-absolute-path": "off",
    "no-useless-constructor": "off",
    "multiline-ternary": "off"
  }
}
